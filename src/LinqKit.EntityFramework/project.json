{
    "version": "1.1.9.0",
    "title": "LinqKit for EntityFramework with Include(...) and IAsync support",
    "description": "LinqKit.EntityFramework is a free set of extensions for LINQ to SQL and Entity Framework power users. Include(...) and IAsync are supported.",
    "authors": [ "Joseph Albahari", "Tomas Petricek", "Scott Smith", "Tuomas Hietanen", "Stef Heyenrath" ],

    "packOptions": {
        "summary": "LinqKit.EntityFramework is a free set of extensions for LINQ to SQL and Entity Framework power users. Include(...) and IAsync are supported.",
        "tags": [ "linq", "EF", "EntityFramework", "Entity", "Framework" ],
        "owners": [ "Stef Heyenrath" ],
        "repository": {
            "type": "git",
            "url": "https://github.com/scottksmith95/LINQKit"
        },
        "projectUrl": "https://github.com/scottksmith95/LINQKit",
        "licenseUrl": "https://github.com/scottksmith95/LINQKit/master/license.txt",
        "releaseNotes": "Rebind base parameter in PredicateBuilder instead of using Invoke."
    },

    "buildOptions": {
        "xmlDoc": true,
        "define": [ "EF" ],
        "compile": {
            "include": [
                "../LinqKit.Core/*.cs",
                "../LinqKit.Core/*/*.cs"
            ],
            "exclude": [
                "../LinqKit.Core/*/AssemblyInfo.cs"
            ]
        },
        "keyFile": "LinqKit.EntityFramework.snk"
    },

    "dependencies": {
        "JetBrains.Annotations": {
            "version": "10.0.0",
            "type": "build"
        },
        "EntityFramework": "6.1.3"
    },

    "frameworks": {
        "net45": {
        }
    }
}