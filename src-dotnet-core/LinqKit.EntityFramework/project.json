            }
        },
        "net45": {
            "frameworkAssemblies": {
            },
            "dependencies": {
                "EntityFramework": "6.1.3"
            }
        },
        "net451": {
            "frameworkAssemblies": {
            },
            "dependencies": {
                "EntityFramework": "6.1.3"
            }
        },
        "net452": {
            "frameworkAssemblies": {
            },
            "dependencies": {
                "EntityFramework": "6.1.3"
            }
        },
        "net46": {
            "frameworkAssemblies": {
            },
            "dependencies": {
                "EntityFramework": "6.1.3"
            }
        },
        "net461": {
            "frameworkAssemblies": {
            },
            "dependencies": {
                "EntityFramework": "6.1.3"
            }
        }
    }
}